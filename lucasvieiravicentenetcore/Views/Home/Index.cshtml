<!-- Contact Section-->
<section class="page-section" id="contact">
    <div class="container">
        <!-- Contact Section Heading-->
        <h2 class="page-section-heading text-center text-uppercase text-secondary mb-0">Entre em contato</h2>
        <!-- Icon Divider-->
        <div class="divider-custom">
            <div class="divider-custom-line"></div>
        </div>
        <!-- Contact Section Form-->
        <div class="row">
            <div class="col-lg-8 mx-auto">
                <!-- To configure the contact form email address, go to mail/contact_me.php and update the email address in the PHP file on line 19.-->
                <form id="contactForm" name="sentMessage" novalidate="novalidate" asp-action="SendEmail" asp-controller="Home">
                    <div class="control-group">
                        <div class="form-group floating-label-form-group controls mb-0 pb-2">
                            <label asp-for="Name"></label>
                            <input class="form-control" id="name" type="text" placeholder="Nome" asp-for="Name" />
                            <span class="help-block text-danger" asp-validation-for="Name"></span>
                        </div>
                    </div>
                    <div class="control-group">
                        <div class="form-group floating-label-form-group controls mb-0 pb-2">
                            <label asp-for="Email"></label>
                            <input class="form-control" id="email" type="email" placeholder="E-mail" asp-for="Email" />
                            <span class="help-block text-danger" asp-validation-for="Email"></span>
                        </div>
                    </div>
                    <div class="control-group">
                        <div class="form-group floating-label-form-group controls mb-0 pb-2">
                            <label asp-for="PhoneNumber"></label>
                            <input class="form-control" id="phone" type="tel" placeholder="Telefone" asp-for="PhoneNumber" />
                            <span class="help-block text-danger" asp-validation-for="PhoneNumber"></span>
                        </div>
                    </div>
                    <div class="control-group">
                        <div class="form-group floating-label-form-group controls mb-0 pb-2">
                            <label asp-for="Body"></label>
                            <textarea class="form-control" id="message" rows="5" placeholder="Mensagem" asp-for="Body"></textarea>
                            <span class="help-block text-danger" asp-validation-for="Body"></span>
                        </div>
                    </div>
                    <br />
                    <div class="form-group"><button class="btn btn-primary btn-xl" id="sendMessageButton" type="submit">Enviar</button></div>
                </form>
            </div>
        </div>
    </div>
</section>

